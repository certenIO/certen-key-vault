{
  "name": "certen-key-vault",
  "version": "1.0.0",
  "description": "Secure key vault and signer browser extension for the Certen Protocol",
  "main": "dist/background.js",
  "scripts": {
    "icons": "node scripts/generate-icons.js",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production && npm run icons",
    "build:dev": "webpack --mode development && npm run icons",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "keywords": [
    "certen",
    "accumulate",
    "blockchain",
    "wallet",
    "browser-extension",
    "key-vault",
    "ed25519",
    "secp256k1"
  ],
  "author": "Certen Protocol",
  "license": "MIT",
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.3.3",
    "@noble/secp256k1": "^2.0.0",
    "@scure/bip32": "^1.3.3",
    "@scure/bip39": "^1.2.1",
    "bech32": "^2.0.0",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.34.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
